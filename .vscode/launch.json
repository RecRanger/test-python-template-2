{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Current File",
      "type": "debugpy",
      "request": "launch",
      "stopOnEntry": true,
      "program": "${file}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Sample API",
      "type": "debugpy",
      "request": "launch",
      "program": "./app/main.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
